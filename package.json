{
  "name": "meal-planner",
  "version": "1.0.0",
  "description": "AI-powered meal planner with BMI calculation and nutrition tracking",
  "main": "server/index.js",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "nodemon server/index.js",
    "client": "cd client && npm start",
    "build": "cd client && npm run build",
    "install-all": "npm install && cd client && npm install"
  },
  "dependencies": {
    "ai": "^3.4.32",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "html2canvas": "^1.4.1",
    "jspdf": "^2.5.2",
    "openai": "^4.28.4"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "nodemon": "^3.0.2"
  },
  "keywords": [
    "meal-planner",
    "nutrition",
    "bmi",
    "ai",
    "openai"
  ],
  "author": "Meal Planner App",
  "license": "MIT"
}
